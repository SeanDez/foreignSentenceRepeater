   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "-c",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/compiled/index.js -c",
         "preLaunchTask": "tsc",
         "outFiles": [
            "${workspaceFolder}/compiled/**/*.js"
         ],
         "sourceMaps": true,
         "smartStep": true,
      }, {
         "type": "node",
         "request": "launch",
         "name": "-b",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/compiled/index.js",
         // "preLaunchTask": "tsc",
         "outFiles": [
            "${workspaceFolder}/compiled/**/*.js"
         ],
         "sourceMaps": true,
         "smartStep": true,
         "internalConsoleOptions": "openOnSessionStart"
      }
   ]
}